// This is Gate One's main settings file.
{
    // "gateone" server-wide settings fall under "*"
    "*": {
        "gateone": { // These settings apply to all of Gate One
            "address": "",
            "base_url": "",
            "ca_certs": null,
            "cache_dir": "/var/lib/gateone/.gateone/cache",
            "certificate": "/etc/gateone/ssl/certificate.pem",
            "cookie_secret": "XXX",
            "debug": false,
            "disable_ssl": false,
            "embedded": false,
            "enable_unix_socket": false,
            "gid": "988",
            "https_redirect": false,
            "js_init": "",
            "keyfile": "/etc/gateone/ssl/keyfile.pem",
            "locale": "en_US",
            "log_file_max_size": 100000000,
            "log_file_num_backups": 10,
            "log_file_prefix": "/var/lib/gateone/.gateone/logs/gateone.log",
            "log_rotate_interval": 1,
            "log_rotate_mode": "size",
            "log_rotate_when": "midnight",
            "log_to_stderr": null,
            "logging": "info",
            "multiprocessing_workers": null,
            "origins": ["localhost:10443", "127.0.0.1:10443", "mini:10443", "192.168.1.5:10443"],
            "pid_file": "/var/lib/gateone/.gateone/gateone.pid",
            "port": 10443,
            "session_dir": "/var/lib/gateone/.gateone/sessions",
            "session_timeout": "5d",
            "syslog_facility": "daemon",
            "uid": "999",
            "unix_socket_mode": "0600",
            "unix_socket_path": "/tmp/gateone.sock",
            "url_prefix": "/",
            "user_dir": "/var/lib/gateone/.gateone",
            "user_logs_max_age": "30d"
        }
    }
}
