#!/usr/bin/env python

"""Backup script"""

import sys
import subprocess


def main():
  cmd = sys.argv[1]
  if cmd == "homeruss":
    homeruss()


def homeruss():
  subprocess.call(["rsync", "-av", "--delete",
                   "/home/russ/task/tasks.tsk",
                   "/mnt/russp/info/task/tasks.tsk"])


if __name__ == "__main__":
  main()
